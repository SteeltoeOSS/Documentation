{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/*/src/*/*.csproj"
          ],
          "src": "sources/v2"
        }
      ],
      "output": "../docs/api/v2",
      "allowCompilationErrors": true,
      "filter": "api-filter-v2.yml",
      "properties": {
        "RunAnalyzers": "false",
        "ProduceReferenceAssembly": "true"
      }
    }
  ]
}

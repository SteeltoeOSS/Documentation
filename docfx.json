{
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/v2/**/toc.yml",
          "api/v3/**/toc.yml",

          "articles/**.md",
          "articles/**/toc.yml",

          "labs/**.md",
          "labs/**/toc.yml",
          
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/amqp.md",
          "**/rabbitmq-binder.md"
        ]
      },
      {
        "files": [
          "api/browser/v2/**.yml"
        ],
        "exclude": [
          "api/browser/v2/all/**.yml"
        ],
        "version": "2.x"
      },
      {
        "files": [
          "api/browser/v3/**.yml"
        ],
        "exclude": [
          "api/browser/v3/all/**.yml"
        ],
        "version": "3.x"
      },
      {
        "files": [
          "api/browser/v2/all/**.yml"
        ],
        "version": "2.all"
      },
      {
        "files": [
          "api/browser/v3/all/**.yml"
        ],
        "version": "3.all"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "fonts/**",
          "articles/**/images/**",
          "api/**/images/**",
          "labs/**/images/**",
          "**.html"
        ]
      }
    ],
    "xref": [
      "_site/xrefmap.yml"
    ],
    "dest": "_site",
    "globalMetadataFiles": ["metadata.json"],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template/steeltoe"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}